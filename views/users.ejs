<% include partials/header %>

<div class="container">
    <h1 class="mt-5 mb-5 text-center">Usuarios</h1>
    <% users.forEach(function(user){ %>
        <div class="bg-success">
            <ul class="list-group">
                <li class="list-group-item active"><%= user.nome %></li>
                <li class="list-group-item"><%= user.sobrenome %></li>
                <li class="list-group-item"><%= user.idade %></li>
            </ul>
        </div>
    <% }) %>
    <button class="btn btn-primary"><a style="color:white" href="/">Retornar</a></button>
</div>

<% include partials/footer %>